{
    "name": "bartlett/monolog-callbackfilterhandler",
    "description": "Monolog handler wrapper that filters records based on a list of callback functions",
    "keywords": ["log", "logging", "logger", "handler"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/llaville/monolog-callbackfilterhandler",
        "issues": "https://github.com/llaville/monolog-callbackfilterhandler/issues"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.0"
    },
    "authors": [
        {
            "name": "Laurent Laville",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bartlett\\Monolog\\Handler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bartlett\\Monolog\\Handler\\Tests\\": "tests/"
        }
    }
}
